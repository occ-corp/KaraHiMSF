<h1>Editing item</h1>

<%= form_for(@item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :evaluation_category_id %><br />
    <%= f.text_field :evaluation_category_id %>
  </p>
  <p>
    <%= f.label :evaluation_item_id %><br />
    <%= f.text_field :evaluation_item_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @item %> |
<%= link_to 'Back', items_path %>
