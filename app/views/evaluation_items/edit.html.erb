<h1>Editing evaluation_item</h1>

<%= form_for(@evaluation_item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @evaluation_item %> |
<%= link_to 'Back', evaluation_items_path %>
